.data

str: .asciiz "Integer: "
str1: .asciiz "Positive\n"
str2: .asciiz "Negative\n"
.text
.globl main
main:

la $a0, str
li $v0, 4				
syscall

li $v0, 5							#read int
syscall	

la $a0, str2				 #exo san default na einai Negative"
sll $t0, $v0, 31
beq $t0, $zero, Neg 	#to 0 vgainei NEG????

la $a0, str1
Neg:
li $v0, 4				
syscall

li $v0, 10	#end program
syscall
