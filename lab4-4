.data
str: .asciiz "# of reps\n"
value: .word 0
.text
.globl main
main:

li $v0, 4  						#print string
la $a0, str
syscall

la $t0, value
li $v0, 5							#read integer
syscall
sw $v0, 0($t0)
lw $t1, 0($t0)

li $t3, 0							# i=0

add $t2, $zero, $t1 	# $t2=  (plhthos epanalhpseon)
again: 

li $a0,'*'
li $v0, 11						#print *
syscall

addi $t3, $t3, 1 			# i=i+1
bne $t2, $t3, again 	# oso i< $t1

exit:
li $v0, 10						#end program
syscall
