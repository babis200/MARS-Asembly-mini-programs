.data

str: .asciiz "Result= "
arr: .word 1, 42, 0, -1, 1, 2, 56

.text

li $t0, 28			#size = 7 alla kathe int katalamvanei 4 theseis mnhmhs ara 4*7=28
li $t1, 0				#i = 0
li $t2, 0				#sum = 0
la $t3, arr    

start_loop:  
  	beq $t0, $t1, end_loop
  	
  	lw $t3, arr($t1)
		add $t2, $t2, $t3
  	
  	addi $t1, $t1, 4    # i +=4
  	 
  	j start_loop  
	end_loop: 

li $v0, 4  #print string
la $a0, str
syscall
	
li $v0, 1  #print int
move $a0, $t2
syscall

li $v0, 10									#end program
syscall